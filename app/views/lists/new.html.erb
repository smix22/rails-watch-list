<div class="bg-primary">
  <div>
    <%= link_to "My Lists", lists_path, class: "btn btn-primary" %>
    <%= link_to "+ New", new_list_path, class: "btn btn-primary" %>
  </div>
</div>

<div class="container m-3 p-3">
  <div class="row">
    <div class="col-6">
      <h2>New List</h2>
      <%= simple_form_for @list do |f| %>
        <%= f.input :name %>
        <%= f.input :photo, as: :file %>
        <%= f.submit "Create List", class: "btn btn-primary" %>
      <% end %>
    </div>
  </div>
</div>
